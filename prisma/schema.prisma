generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Musica {
  id        Int    @id @default(autoincrement())
  nome      String @unique
  artista   String
  categoria String
  letra     String
  cifra     String
  url       String
  urlQrCode String
}
